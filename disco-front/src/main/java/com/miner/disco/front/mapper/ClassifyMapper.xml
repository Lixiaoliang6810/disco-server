<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.miner.disco.front.dao.ClassifyMapper">

    <select id="queryAllEffective" resultType="com.miner.disco.front.model.response.ClassifyListResponse">
