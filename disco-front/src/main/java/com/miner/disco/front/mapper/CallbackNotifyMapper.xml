<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.miner.disco.front.dao.CallbackNotifyMapper">

    <select id="queryBySnAndType" resultType="com.miner.disco.pojo.CallbackNotify">
        SELECT * FROM ds_callback_notify cn WHERE cn.callback_type = #{type} AND cn.callback_sn  = #{sn}
    </select>

</mapper>